{
  "version": 2,
  "name": "dashboard-server",
  "env": {
    "JWT_SECRET": "@jwt-secret",
    "JWT_ISSUER": "@jwt-issuer",
    "JWT_AUDIENCE": "@jwt-audience"
  },
  "routes": [
    { "src": "/(.*)", "dest": "/lib/index.js" }
  ],
  "builds": [
    { "src": "lib/index.js", "use": "@now/node" }
  ]
}
